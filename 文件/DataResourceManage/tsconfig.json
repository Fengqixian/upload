{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dragRelation.vue"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "target": "es5",
    "experimentalDecorators": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
